export PATH="/bin:/sbin:/usr/bin:/usr/sbin:/progs:/files/x86_64-linux-musl-native/bin:/files/Python-3.13.1"

if [ "$PS1" ]; then
	if [ "`id -u`" -eq 0 ]; then
		export PS1='# '
	else
		export PS1='$ '
	fi
fi

export EDITOR='/bin/vi'

# Source configuration files from /etc/profile.d
for i in /etc/profile.d/*.sh ; do
	if [ -r "$i" ]; then
		. $i
	fi
done
unset i
